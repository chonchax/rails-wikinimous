<%= form_with model: @article do |f| %>
  <div id="formulaire">
    <div class="mb-3">
      <%= f.label :title %>
      <%= f.text_field :title, class: "form-control"%>
    </div>
    <div class=" mb-3">
    <%= f.label :content %>
    <%= f.text_field :content, class: "form-control" %>
    </div>
    <%= f.submit class: "btn btn-primary"%>
  <% end %>
  </div>
